{
    "languageserver": {
        "golang": {
          "command": "gopls",
          "rootPatterns": ["go.mod", ".git/"],
          "filetypes": ["go"]
        }
      },
      "suggest.noselect": false,
      "suggest.snippetIndicator": "â–º",
      "suggest.acceptSuggestionOnCommitCharacter": true,
      "suggest.minTriggerInputLength": 2,
      "coc.source.dictionary.enable": true,
      "coc.source.dictionary.priority": 9,
      "coc.source.tag.enable": true,
      "coc.source.tag.priority": 8,
      "coc.source.word.enable": true,
      "coc.source.word.priority": 11,
      "snippets.priority": 7,
      "snippets.ultisnips.directories": [
                "UltiSnips",
                "gosnippets/UltiSnips"
       ]
}
